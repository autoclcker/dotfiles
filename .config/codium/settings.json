{
    "window.menuBarVisibility": "toggle",
    "vim.normalModeKeyBindings": [
        {
            "before": ["<leader>", "/"],
            "after": ["<leader>", "<leader>", "s"]
        },
        {
            "before": ["<leader>", "f"],
            "after": ["<leader>", "<leader>", "f"]
        },
        {
            "before": ["<leader>", "F"],
            "after": ["<leader>", "<leader>", "F"]
        },
        {
            "before": ["<leader>", "t"],
            "after": ["<leader>", "<leader>", "t"]
        },
        {
            "before": ["<leader>", "T"],
            "after": ["<leader>", "<leader>", "T"]
        },
        {
            "before": ["<leader>", "w"],
            "after": ["<leader>", "<leader>", "w"]
        },
        {
            "before": ["<leader>", "b"],
            "after": ["<leader>", "<leader>", "b"]
        },
        {
            "before": ["<leader>", "j"],
            "after": ["<leader>", "<leader>", "j"]
        },
        {
            "before": ["<leader>", "k"],
            "after": ["<leader>", "<leader>", "k"]
        },
        {
            "before": ["<leader>", "n"],
            "commands": ["editor.action.marker.next"],
        },
        {
            "before": ["<leader>", "p"],
            "commands": ["editor.action.marker.prev"],
        },
        {
            "before": ["leader", "v"],
            "commands": ["codeium.pasteAndTranslate"]
        },
        {
            "before": ["leader", "c"],
            "commands": ["copyRelativeFilePath"],
            "silent": true
        },
        {
            "before": ["<leader>", "m"],
            "commands": [ "bookmarks.toggle" ],
            "silent": true
        },
        {
            "before": ["<leader>", "'"],
            "commands": [
                "bookmarks.listFromAllFiles"
            ]
        },
        {
            "before": ["<leader>", ","],
            "commands": ["editor.fold"],
            "silent": true
        },
        {
            "before": ["<leader>", "."],
            "commands": ["editor.unfold"],
            "silent": true
        },
        {
            "before": ["<leader>", "1"],
            "commands": ["editor.foldLevel1"],
            "silent": true
        },
        {
            "before": ["<leader>", "2"],
            "commands": ["editor.foldLevel2"],
            "silent": true
        },
        {
            "before": ["<leader>", "3"],
            "commands": ["editor.foldLevel3"],
            "silent": true
        },
        {
            "before": ["<leader>", "4"],
            "commands": ["editor.foldLevel4"],
            "silent": true
        },
        {
            "before": ["<leader>", "5"],
            "commands": ["editor.foldLevel5"],
            "silent": true
        },
        {
            "before": ["<leader>", "6"],
            "commands": ["editor.foldLevel6"],
            "silent": true
        },
        {
            "before": ["<leader>", "7"],
            "commands": ["editor.foldLevel7"],
            "silent": true
        },
        {
            "before": ["<leader>", "0"],
            "commands": ["editor.unfoldAll"],
            "silent": true
        },
        {
            "before": [
                ">"
            ],
            "commands": [
                "editor.action.indentLines"
            ],
            "silent": true
        },
        {
            "before": [
                "<"
            ],
            "commands": [
                "editor.action.outdentLines"
            ],
            "silent": true
        },
        {
            "before": ["g", "p", "d"],
            "commands": ["editor.action.peekDefinition"]
        },
        {
            "before": ["g", "h"],
            "commands": ["editor.action.showDefinitionPreviewHover"]
        },
        {
            "before": ["g", "s"],
            "commands": ["editor.action.referenceSearch.trigger"]
        },
        {
            "before": ["g", "i"],
            "commands": ["editor.action.goToImplementation"]
        },
        {
            "before": ["g", "p", "i"],
            "commands": ["editor.action.peekImplementation"]
        },
        {
            "before": ["g", "t"],
            "commands": ["editor.action.goToTypeDefinition"]
        },
        {
            "before": ["g", "p", "t"],
            "commands": ["editor.action.peekTypeDefinition"]
        },
        {
            "before": ["C-b"],
            "commands": ["cursorPageUp"],
            "silent": true
        },
        {
            "before": ["C-f"],
            "commands": ["cursorPageDown"],
            "silent": true
        },
        {
            "before": ["C-y"],
            "commands": ["scrollLineUp", "scrollLineUp", "cursorUp"],
            "silent": true
        },
        {
            "before": ["C-e"],
            "commands": ["scrollLineDown", "scrollLineDown", "cursorDown"],
            "silent": true
        },
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": ["<leader>", "h"],
            "commands": ["codeium.explainCodeBlock"]
        },
        {
            "before": ["<leader>", "r"],
            "commands": ["codeium.refactorCodeBlock"]
        },
        {
            "before": ["Y"],
            "after": ["\"", "*", "y"]
        },
    ],
    "cmake.showOptionsMovedNotification": false,
}
